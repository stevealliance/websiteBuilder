{
  "name": "website-builder",
  "version": "1.0.21",
  "license": "CC-BY-3.0",
  "description": "edit data for a templates",
  "homepage": "http://torben.website/websiteBuilder",
  "documentationWebsite": {
    "languages": [
      "deDE",
      "enUS"
    ],
    "name": "websiteBuilder",
    "tagline": "agile template maintenance<!--deDE:agile Template Pflege-->",
    "trackingCode": "UA-40192634-27"
  },
  "keywords": [
    "template",
    "website",
    "builder",
    "editor",
    "data",
    "representation"
  ],
  "files": [
    "angular.js",
    "angular.compiled.js",
    "editorAssets.compiled",
    "index.js",
    "index.css",
    "index.compiled.css"
  ],
  "main": "index.compiled.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/websiteBuilder"
  },
  "scripts": {
    "build": "webOptimizer build",
    "build:dll": "webOptimizer build:dll",
    "clear": "webOptimizer clear",
    "document": "webOptimizer document",
    "export": "webOptimizer build '{development:{tool:false},module:{preprocessor:{javaScript:{options:{plugins:{__remove__:\"transform-runtime\"}}}}}}'",
    "lint": "webOptimizer check:type; webOptimizer lint",
    "prepare": "npm run build",
    "serve": "webOptimizer serve",
    "start": "npm run serve",
    "test": "webOptimizer test",
    "test:browser": "webOptimizer test:browser",
    "update": "npm version patch && npm publish",
    "update:documentation": "documentationWebsite --log-level info",
    "watch": "webOptimizer build --watch",
    "watch:dll": "webOptimizer build:dll --watch"
  },
  "dependencies": {
    "clientnode": "^1.0.251",
    "ejs": "^2.5.7",
    "json-editor": "^0.7.28",
    "tinymce": "^4.7.1"
  },
  "peerDependencies": {
    "angular-generic": "^1.0.282",
    "@angular/animations": "^4.2.6",
    "@angular/common": "^4.2.6",
    "@angular/compiler": "^4.2.6",
    "@angular/core": "^4.2.6",
    "@angular/forms": "^4.2.6",
    "@angular/http": "^4.2.6",
    "@angular/material": "^2.0.0-beta.8",
    "@angular/platform-browser": "^4.2.6",
    "@angular/platform-browser-dynamic": "^4.2.6",
    "@angular/router": "^4.2.6",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/animations": "^4.4.5",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/common": "^4.4.5",
    "@angular/compiler": "^4.4.5",
    "@angular/core": "^4.4.5",
    "@angular/forms": "^4.4.5",
    "@angular/http": "^4.4.5",
    "@angular/material": "^2.0.0-beta.12",
    "@angular/platform-browser": "^4.4.5",
    "@angular/platform-browser-dynamic": "^4.4.5",
    "@angular/router": "^4.4.5",
    "angular-generic": "^1.0.357",
    "babel-eslint": "^8.0.1",
    "babel-plugin-angular2-annotations": "^5.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "documentation-website": "^1.0.67",
    "eslint": "^4.9.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.36.0",
    "eslint-plugin-jsdoc": "^3.1.1",
    "flow-bin": "^0.57.3",
    "jsdoc": "^3.5.2",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.2",
    "source-map-support": "^0.5.0",
    "stylelint": "^8.2.0",
    "weboptimizer": "^1.0.381",
    "webpack": "^3.8.0",
    "webpack-dev-server": "^2.9.2",
    "zone.js": "^0.8.18"
  },
  "webOptimizer": {
    "injection": {
      "external": {
        "implicit": {
          "pattern": {
            "exclude": {
              "__append__": {
                "__evaluate__": "/^clientnode\\/test(?:\\.compiled)?(?:\\.js)?$/"
              }
            }
          }
        }
      },
      "internal": {
        "angular": {
          "__evaluate__": "self.givenCommandLineArguments.length && ['test', 'test:browser'].includes(self.givenCommandLineArguments[2]) ? [] : './angular'"
        },
        "index": {
          "__evaluate__": "(self.givenCommandLineArguments.length && ['build:dll', 'serve', 'test:browser', 'watch:dll'].includes(self.givenCommandLineArguments[2]) ? [`webpack-dev-server/client?http${self.development.server.cert ? 's' : ''}://0.0.0.0:${self.development.server.port}`] : []).concat(self.givenCommandLineArguments.length && ['test', 'test:browser'].includes(self.givenCommandLineArguments[2]) ? './test' : './index')"
        },
        "testApplication": {
          "__evaluate__": "self.givenCommandLineArguments.length && self.givenCommandLineArguments[2] === 'build' ? [] : './testApplication'"
        },
        "vendor": {
          "__evaluate__": "self.debug && 2 < self.givenCommandLineArguments.length && ['build:dll', 'serve', 'test:browser', 'watch:dll'].includes(self.givenCommandLineArguments[2]) ? ['babel-polyfill', 'clientnode', 'hammerjs', 'reflect-metadata', 'zone.js', 'rxjs', '@angular/animations', '@angular/common', '@angular/core', '@angular/http', '@angular/material', '@angular/platform-browser', '@angular/platform-browser-dynamic', '@angular/router', 'angular-generic'].concat(self.givenCommandLineArguments[2] === 'serve' ? [] : ['script!qunitjs', 'weboptimizer/browserAPI']) : []"
        }
      }
    },
    "inPlace": {
      "externalLibrary": {
        "cascadingStyleSheet": true,
        "javaScript": true
      }
    },
    "module": {
      "aliases": {
        "clientnode/test$": "clientnode/test.compiled"
      },
      "optimizer": {
        "babelMinify": null,
        "data": {
          "exclude": "true"
        },
        "font": {
          "eot": {
            "exclude": "true"
          },
          "svg": {
            "exclude": "true"
          },
          "ttf": {
            "exclude": "true"
          },
          "woff": {
            "exclude": "true"
          }
        },
        "image": {
          "exclude": "true"
        }
      },
      "preprocessor": {
        "javaScript": {
          "options": {
            "plugins": {
              "__prepend__": [
                "angular2-annotations",
                "transform-decorators-legacy"
              ],
              "__remove__": "transform-runtime"
            },
            "presets": {
              "__evaluate__": "(self.targetTechnology === 'node' ? ['es2017-node7'] : [['modern-browsers', {loose: true}], 'es2017']).concat('stage-0')"
            }
          }
        }
      },
      "replacements": {
        "context": [
          [
            "/angular(\\\\|\\/)core(\\\\|\\/)(esm(\\\\|\\/)src|src)(\\\\|\\/)linker/",
            "__dirname"
          ]
        ],
        "normal": {
          "^DOESNT WORK: rxjs(?:/.+)?$": "rxjs/bundles/Rx.min.js",
          "^DOESNT WORK IN NODE: zone\\.js(?:/.+)?$": "zone.js/dist/zone.min.js"
        }
      },
      "skipParseRegularExpressions": {
        "__evaluate__": "/(?:^|\\/)(?:(?:babel-polyfill\\/dist\\/polyfill)|(?:webpack\\/buildin\\/global)|(?:webpack\\/buildin\\/module))(?:\\.min)?\\.js(?:\\?.+|$)/"
      }
    }
  }
}
